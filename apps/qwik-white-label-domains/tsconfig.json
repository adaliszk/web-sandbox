{
    "extends": "@adaliszk/typescript",
    "include": ["vite.*", "src/**/*.tsx", "src/**/*"],
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "allowImportingTsExtensions": true,
        "esModuleInterop": true,
        "jsx": "react-jsx",
        "jsxImportSource": "@builder.io/qwik",
        "types": ["vite/client"],
        "baseUrl": "src",
        "outDir": "dist"
    }
}
